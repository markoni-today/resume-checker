{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "python/api_handler.py",
      "use": "@vercel/python"
    }
  ],
  "functions": {
    "src/app/api/analyze/route.ts": {
      "maxDuration": 30
    },
    "python/api_handler.py": {
      "maxDuration": 25
    }
  },
  "routes": [
    {
      "src": "/api/analyze",
      "dest": "/api/analyze"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9"
  },
  "installCommand": "npm install && pip install -r requirements.txt"
}